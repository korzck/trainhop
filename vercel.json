{
    "trailingSlash": false,
    "functions": {
        "api/test.js": {
            "memory": 3008,
            "maxDuration": 30
        },
        "api/*.js": {
            "memory": 3008,
            "maxDuration": 30
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api/index.go"
        }
    ]
}
